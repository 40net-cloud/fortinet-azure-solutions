config vpn ipsec phase1-interface
edit "toazure1"
set interface "port2"
set ike-version 2
set keylife 28800
set peertype any
set proposal aes256-sha1
set dhgrp 2
set remote-gw 51.105.169.47
set psksecret EyYqyEZsNdYfXMtYv2iTDqnKUBM1Yg1i
next
edit "toazure2"
set interface "port2"
set ike-version 2
set keylife 28800
set peertype any
set proposal aes256-sha1
set dhgrp 2
set remote-gw 51.105.169.103
set psksecret EyYqyEZsNdYfXMtYv2iTDqnKUBM1Yg1i
next
end
config vpn ipsec phase2-interface
edit "toazure1"
set phase1name "toazure1"
set proposal aes256-sha1
set dhgrp 2
set keylifeseconds 3600
next
edit "toazure2"
set phase1name "toazure2"
set proposal aes256-sha1
set dhgrp 2
set keylifeseconds 3600
next
end
config system settings
set allow-subnet-overlap enable
end
config system interface
edit "toazure1"
set vdom "root"
set ip 169.254.111.1 255.255.255.255
set type tunnel
set remote-ip 172.16.0.135 255.255.255.255
set snmp-index 4
set interface "port2"
next
edit "toazure2"
set vdom "root"
set ip 169.254.111.1 255.255.255.255
set type tunnel
set remote-ip 172.16.0.134 255.255.255.255
set snmp-index 5
set interface "port2"
next
end
config router bgp
set as 65516
set router-id 169.254.24.25
config neighbor
edit "172.16.0.135"
set remote-as 65515
next
edit "172.16.0.134"
set remote-as 65515
next
end
config network
edit 1
set prefix 172.16.254.0 255.255.255.0
next
end
config redistribute "connected"
set status enable
end
config redistribute "rip"
end
config redistribute "ospf"
end
config redistribute "static"
end
config redistribute "isis"
end
config redistribute6 "connected"
end
config redistribute6 "rip"
end
config redistribute6 "ospf"
end
config redistribute6 "static"
end
config redistribute6 "isis"
end
