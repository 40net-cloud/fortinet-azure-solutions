      {
        "name": "ipconfig1",
        "properties": {
          "primary": true,
          "privateIPAddress": "[variables('sn1IPfmgA')]",
          "privateIPAllocationMethod": "Static",
          "publicIPAddress": "[if(not(equals(parameters('publicIP1NewOrExisting'), 'none')), variables('publicIPAddress1Id'), json('null'))]",
          "subnet": {
            "id": "[variables('subnet1Id')]"
          }
        }
      },
      {
        "name": "vip-priv",
        "properties": {
