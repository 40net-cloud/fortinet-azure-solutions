    "instanceType": {
      "type": "string",
      "defaultValue": "Standard_D4s_v3",
      "allowedValues": [
        "Standard_DS4_v2",
        "Standard_DS5_v2",
        "Standard_D4_v3",
        "Standard_D8_v3",
        "Standard_D16_v3",
